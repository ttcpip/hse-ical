{
  "name": "hse-ical",
  "version": "2.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "node dist/index.js",
    "build": "bun x tsc",
    "dev": "concurrently -k -i -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,cyan.bold\" \"bun run watch-js\" \"bun run watch-ts\"",
    "watch-ts": "bun run tsc -w",
    "watch-js": "nodemon dist/index.js"
  },
  "dependencies": {
    "dedent": "^1.5.3",
    "dotenv": "^14.3.2",
    "envalid": "^8.0.0",
    "got": "^11.8.6",
    "ical-generator": "^3.6.1",
    "lru-cache": "^6.0.0",
    "luxon": "^2.5.2"
  },
  "devDependencies": {
    "@types/lru-cache": "^5.1.1",
    "@types/luxon": "^2.4.0",
    "@types/node": "^17.0.45",
    "concurrently": "^7.6.0",
    "dayjs": "^1.11.13",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "nodemon": "^2.0.22",
    "rimraf": "^6.0.1",
    "rrule": "^2.8.1",
    "typescript": "^4.9.5"
  }
}